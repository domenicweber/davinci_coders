<h2>Cars Listing</h2>

  <table id = "car_table">
    <thead>
    <tr>
      <th>Make</th>
      <th>Model</th>
      <th>Year</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @car.each do |car| %>
      <tr>
        <td><%= car.make %></td>
        <td><%= car.model %></td>
        <td><%= car.year %></td>
        <td>$<%= car.price %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
<br>


<%= link_to 'New Car' , new_car_path %>



